{
  "name": "429chain",
  "version": "0.1.0",
  "type": "module",
  "description": "OpenAI-compatible proxy that waterfalls requests through provider chains on rate limits",
  "main": "dist/index.mjs",
  "bin": {
    "429chain": "dist/cli.mjs"
  },
  "files": [
    "dist/",
    "ui/dist/",
    "config/config.example.yaml"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npm run build:backend && npm run build:ui",
    "build:backend": "tsdown src/cli.ts src/index.ts --format esm --dts",
    "build:ui": "cd ui && npm run build",
    "start": "node dist/index.mjs",
    "prepublishOnly": "npm test && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "openai",
    "proxy",
    "rate-limit",
    "waterfall",
    "llm",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "better-sqlite3": "^12.6.2",
    "hono": "^4.11.7",
    "ms": "^2.1.3",
    "nanoid": "^5.1.6",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/ms": "^2.1.0",
    "@types/node": "^25.2.0",
    "tsdown": "^0.20.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
