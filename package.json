{
  "name": "429chain",
  "version": "0.1.0",
  "type": "module",
  "description": "OpenAI-compatible proxy that waterfalls requests through provider chains on rate limits",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsdown src/index.ts --format esm --dts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "openai",
    "proxy",
    "rate-limit",
    "waterfall",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "hono": "^4.11.7",
    "ms": "^2.1.3",
    "nanoid": "^5.1.6",
    "pino": "^10.3.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/ms": "^2.1.0",
    "@types/node": "^25.2.0",
    "tsdown": "^0.20.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
